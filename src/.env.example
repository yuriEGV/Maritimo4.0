# Environment example for Maritimo (local/testing)

# MongoDB connection URI
MONGO_URI=mongodb://localhost:27017/maritimo_test

# Server port
PORT=5000

# Webpay / Transbank shared secret used to verify webhook signatures
# For local testing you can leave this empty to bypass verification.
WEBPAY_SECRET=your_webpay_shared_secret
TRANSBANK_SHARED_SECRET=your_transbank_shared_secret

# Optional keys for other providers used by the app
FLOW_API_KEY=
MERCADOLIBRE_ACCESS_TOKEN=

NODE_ENV=development
